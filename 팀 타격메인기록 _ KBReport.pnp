	<script type="text/javascript">
	$(function() {
		  $(window).hashchange( function(){
		    var hash = location.hash.replace( /^#\//, '' );
		    if ( hash != "#" )	list(hash);
		  });
		  $(window).hashchange();
		  
		  $('.t-f-submit').bind('click', function(e) { 
			  $("#order").val("oWAR");
			  $("#orderType").val("DESC");
			  $("#searchFrm").submit();
			  e.preventDefault();
			  });
	});
	
	function changRow(row) {
		$("#rows").val(row);
		list("1");
	}
	
	function list(page) {
		if (!page.match(/^\d+$/ig)) page = 1;
		$.ajax({
			type: "POST",
			data : $("#searchFrm").serialize()+"&page="+page,
			url : '/teams/list/ajax',
			success : function(result){
				$("#resultListDiv").html(result);
			},
			error : function(result){
				alert("리스트 조회를 실패하였습니다.");
			}
		});
	}
	
	function f_order(order, type) {
		$("#order").val(order);
		if (""==order) {
			if ( ""=="DESC" ) type="ASC";
			else type="DESC";
		}
		$("#orderType").val(type);
		$("#searchFrm").submit();
	}
	
	</script>

